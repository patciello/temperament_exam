{
  "expo": {
    "name": "Teste de Temperamento",
    "slug": "teste-de-temperamento",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",

    "owner": "patciello",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },

      "eas": {
        "projectId": "12f2e7da-3fb3-45c3-9d30-58abc8bf58ff",
        "build": {
          "experimental": {
            "android": {
              "buildType": "apk"
            }
          }
        }
      }
    },
    "android": {
      "package": "com.temperamentotest.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "versionCode": 1,
      "permissions": [],
      "softwareKeyboardLayoutMode": "pan"
    }
  }
}
